() => {\n      const ctx = document.getElementById('forecast-chart').getContext('2d');\n      new Chart(ctx, {\n        type: 'bar',\n        data: {\n          labels: forecastData.value.map(item => item.date),\n          datasets: [{\n            label: 'Temperature (°F)',\n            data: forecastData.value.map(item => item.temp),\n            backgroundColor: 'rgba(75, 192, 192, 0.2)',\n            borderColor: 'rgba(75, 192, 192, 1)',\n            borderWidth: 1\n          }]\n        },\n        options: {\n          responsive: true,\n          scales: {\n            y: {\n              beginAtZero: true\n            }\n          }\n        }\n      });\n    };\n\n    return {\n      forecastData,\n      errorMessage,\n    };\n  }\n});\n</script>\n\n<style scoped>\n\n\n.five-day-forecast {\n  padding: 20px;\n  text-align: center;\n}\n\n.error-message {\n  color: red;\n  margin-top: 20px;\n}\n</style>\n\n\n\n"],"mappings":";AAYA,SAASA,eAAe,EAAEC,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACrD,SAASC,KAAI,QAAS,UAAU;AAEhC,eAAeH,eAAe,CAAC;EAC7BI,IAAI,EAAE,iBAAiB;EACvBC,KAAK,EAAE,CAAC,UAAU,CAAC;EACnBC,KAAKA,CAACD,KAAK,EAAE;IACX,MAAME,YAAW,GAAIN,GAAG,CAAC,EAAE,CAAC;IAC5B,MAAMO,YAAW,GAAIP,GAAG,CAAC,EAAE,CAAC;;IAE5B;IACAC,SAAS,CAAC,MAAM;MACdO,iBAAiB,CAAC,CAAC;IACrB,CAAC,CAAC;IAEF,MAAMA,iBAAgB,GAAIA,CAAA,KAAM;MAC9BC,KAAK,CAAC,sDAAsDL,KAAK,CAACM,QAAQ,8DAA8D,EACrIC,IAAI,CAAEC,GAAG,IAAKA,GAAG,CAACC,IAAI,CAAC,CAAC,EACxBF,IAAI,CAAEG,IAAI,IAAK;QACd,IAAIA,IAAI,CAACC,GAAE,KAAM,KAAK,EAAE;UACtBR,YAAY,CAACS,KAAI,GAAI,gCAAgC;UACrD;QACF;QACAV,YAAY,CAACU,KAAI,GAAIF,IAAI,CAACG,IAAI,CAACC,GAAG,CAACC,IAAG,KAAM;UAC1CC,IAAI,EAAE,IAAIC,IAAI,CAACF,IAAI,CAACG,EAAC,GAAI,IAAI,CAAC,CAACC,kBAAkB,CAAC,CAAC;UACnDC,IAAI,EAAEL,IAAI,CAACM,IAAI,CAACD;QAClB,CAAC,CAAC,CAAC;QACHE,WAAW,CAAC,CAAC;MACf,CAAC,EACAC,KAAK,CAAEC,GAAG,IAAK;QACdrB,YAAY,CAACS,KAAI,GAAI,+BAA+B;QACpDa,OAAO,CAACC,KAAK,CAACF,GAAG,CAAC;MACpB,CAAC,CAAC;IACN,CAAC;IAED,MAAMF,WAAU,GAAIA,CAAA,KAAM;MACxB,MAAMK,GAAE,GAAIC,QAAQ,CAACC,cAAc,CAAC,gBAAgB,CAAC,C