{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport \"core-js/modules/esnext.iterator.map.js\";\nexport default {\n  name: 'FiveDayForecast',\n  props: {\n    location: {\n      type: String,\n      required: true\n    }\n  },\n  data() {\n    return {\n      forecastData: [],\n      errorMessage: ''\n    };\n  },\n  mounted() {\n    this.fetchForecastData();\n  },\n  methods: {\n    // Fetch 5-day forecast data from the OpenWeather API\n    fetchForecastData() {\n      const apiKey = '4df475f18aaaf4aba5fb2eb54246d4f8'; // Replace with your OpenWeather API key\n      const url = `https://api.openweathermap.org/data/2.5/forecast?q=${this.location}&units=imperial&appid=${apiKey}`;\n      fetch(url).then(res => res.json()).then(data => {\n        if (data.cod !== '200') {\n          this.errorMessage = 'Unable to fetch forecast data.';\n          return;\n        }\n\n        // Filter data for 18:00:00 for each day\n        const dailyData = data.list.filter(reading => reading.dt_txt.includes(\"18:00:00\"));\n        this.forecastData = dailyData.map(item => ({\n          date: new Date(item.dt * 1000).toLocaleDateString('en-US', {\n            weekday: 'long'\n          }),\n          weather: item.weather[0].main,\n          high: item.main.temp_max,\n          low: item.main.temp_min\n        }));\n      }).catch(err => {\n        this.errorMessage = 'Error fetching forecast data.';\n        console.error(err);\n      });\n    },\n    // Navigate back using Vue Router\n    goBack() {\n      this.$router.go(-1);\n    }\n  }\n};","map":{"version":3,"names":["name","props","location","type","String","required","data","forecastData","errorMessage","mounted","fetchForecastData","methods","apiKey","url","fetch","then","res","json","cod","dailyData","list","filter","reading","dt_txt","includes","map","item","date","Date","dt","toLocaleDateString","weekday","weather","main","high","temp_max","low","temp_min","catch","err","console","error","goBack","$router","go"],"sources":["/Users/marianfoster/vue-weather/src/components/FiveDayForecast.vue"],"sourcesContent":["<template>\n  <div class=\"forecast-container\">\n    <h2>{{ location }} - 5 Day Forecast</h2>\n\n    <!-- Display forecast data in a table -->\n    <table v-if=\"forecastData.length\" class=\"forecast-table\">\n      <thead>\n        <tr>\n          <th>Day</th>\n          <th>Weather</th>\n          <th>High (°F)</th>\n          <th>Low (°F)</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"day in forecastData\" :key=\"day.date\">\n          <td>{{ day.date }}</td>\n          <td>{{ day.weather }}</td>\n          <td>{{ day.high }}</td>\n          <td>{{ day.low }}</td>\n        </tr>\n      </tbody>\n    </table>\n\n    <!-- Error or loading message -->\n    <p v-else-if=\"errorMessage\">{{ errorMessage }}</p>\n    <p v-else>Loading forecast...</p>\n\n    <!-- Back button to navigate back -->\n    <button class=\"back-btn\" @click=\"goBack\">Back</button>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'FiveDayForecast',\n  props: {\n    location: {\n      type: String,\n      required: true,\n    },\n  },\n  data() {\n    return {\n      forecastData: [],\n      errorMessage: '',\n    };\n  },\n  mounted() {\n    this.fetchForecastData();\n  },\n  methods: {\n    // Fetch 5-day forecast data from the OpenWeather API\n    fetchForecastData() {\n      const apiKey = '4df475f18aaaf4aba5fb2eb54246d4f8'; // Replace with your OpenWeather API key\n      const url = `https://api.openweathermap.org/data/2.5/forecast?q=${this.location}&units=imperial&appid=${apiKey}`;\n      \n      fetch(url)\n        .then((res) => res.json())\n        .then((data) => {\n          if (data.cod !== '200') {\n            this.errorMessage = 'Unable to fetch forecast data.';\n            return;\n          }\n          \n          // Filter data for 18:00:00 for each day\n          const dailyData = data.list.filter((reading) => reading.dt_txt.includes(\"18:00:00\"));\n          this.forecastData = dailyData.map((item) => ({\n            date: new Date(item.dt * 1000).toLocaleDateString('en-US', { weekday: 'long' }),\n            weather: item.weather[0].main,\n            high: item.main.temp_max,\n            low: item.main.temp_min,\n          }));\n        })\n        .catch((err) => {\n          this.errorMessage = 'Error fetching forecast data.';\n          console.error(err);\n        });\n    },\n    // Navigate back using Vue Router\n    goBack() {\n      this.$router.go(-1);\n    },\n  },\n};\n</script>\n<style scoped>\n\n</style>\n\n\n\n\n\n\n\n\n\n"],"mappings":";;;AAkCA,eAAe;EACbA,IAAI,EAAE,iBAAiB;EACvBC,KAAK,EAAE;IACLC,QAAQ,EAAE;MACRC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,EAAE;MAChBC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,iBAAiB,CAAC,CAAC;EAC1B,CAAC;EACDC,OAAO,EAAE;IACP;IACAD,iBAAiBA,CAAA,EAAG;MAClB,MAAME,MAAK,GAAI,kCAAkC,EAAE;MACnD,MAAMC,GAAE,GAAI,sDAAsD,IAAI,CAACX,QAAQ,yBAAyBU,MAAM,EAAE;MAEhHE,KAAK,CAACD,GAAG,EACNE,IAAI,CAAEC,GAAG,IAAKA,GAAG,CAACC,IAAI,CAAC,CAAC,EACxBF,IAAI,CAAET,IAAI,IAAK;QACd,IAAIA,IAAI,CAACY,GAAE,KAAM,KAAK,EAAE;UACtB,IAAI,CAACV,YAAW,GAAI,gCAAgC;UACpD;QACF;;QAEA;QACA,MAAMW,SAAQ,GAAIb,IAAI,CAACc,IAAI,CAACC,MAAM,CAAEC,OAAO,IAAKA,OAAO,CAACC,MAAM,CAACC,QAAQ,CAAC,UAAU,CAAC,CAAC;QACpF,IAAI,CAACjB,YAAW,GAAIY,SAAS,CAACM,GAAG,CAAEC,IAAI,KAAM;UAC3CC,IAAI,EAAE,IAAIC,IAAI,CAACF,IAAI,CAACG,EAAC,GAAI,IAAI,CAAC,CAACC,kBAAkB,CAAC,OAAO,EAAE;YAAEC,OAAO,EAAE;UAAO,CAAC,CAAC;UAC/EC,OAAO,EAAEN,IAAI,CAACM,OAAO,CAAC,CAAC,CAAC,CAACC,IAAI;UAC7BC,IAAI,EAAER,IAAI,CAACO,IAAI,CAACE,QAAQ;UACxBC,GAAG,EAAEV,IAAI,CAACO,IAAI,CAACI;QACjB,CAAC,CAAC,CAAC;MACL,CAAC,EACAC,KAAK,CAAEC,GAAG,IAAK;QACd,IAAI,CAAC/B,YAAW,GAAI,+BAA+B;QACnDgC,OAAO,CAACC,KAAK,CAACF,GAAG,CAAC;MACpB,CAAC,CAAC;IACN,CAAC;IACD;IACAG,MAAMA,CAAA,EAAG;MACP,IAAI,CAACC,OAAO,CAACC,EAAE,CAAC,CA