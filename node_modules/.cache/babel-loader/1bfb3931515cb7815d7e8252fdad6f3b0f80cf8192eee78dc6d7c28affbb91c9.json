ocessForecastData(newData);\n        }\n      }\n    }\n  },\n  methods: {\n    processForecastData(data) {\n      this.chartData.labels = data.map(item => item.day);\n      this.chartData.datasets[0].data = data.map(item => item.high);\n      this.chartData.datasets[1].data = data.map(item => item.low);\n    },\n    goBack() {\n      this.$router.go(-1); // Go back to the previous page\n    }\n  }\n};","map":{"version":3,"names":["Bar","Chart","ChartJS","Title","Tooltip","Legend","BarElement","CategoryScale","LinearScale","register","name","components","props","forecastData","type","Array","default","location","String","data","chartData","labels","datasets","label","backgroundColor","borderColor","borderWidth","chartOptions","responsive","plugins","legend","position","tooltip","callbacks","tooltipItem","dataset","raw","scales","x","title","display","text","y","beginAtZero","watch","immediate","handler","newData","length","processForecastData","methods","map","item","day","high","low","goBack","$router","go"],"sources":["/Users/marianfoster/vue-weather/src/views/5Day.vue"],"sourcesContent":["<template>\n  <div class=\"forecast-container\">\n    <h2>{{ location }} - 5-Day Forecast</h2>\n    <div v-if=\"forecastData && forecastData.length > 0\">\n      <Bar \n        :data=\"chartData\" \n        :options=\"chartOptions\" \n      />\n    </div>\n    <div v-else>\n      <p>Loading forecast...</p>\n    </div>\n    <button class=\"back-button\" @click=\"goBack\">Back</button>\n  </div>\n</template>\n\n<script>\nimport { Bar } from 'vue-chartjs';\nimport {\n  Chart as ChartJS,\n  Title,\n  Tooltip,\n  Legend,\n  BarElement,\n  CategoryScale,\n  LinearScale,\n} from 'chart.js';\n\nChartJS.register(\n  Title, \n  Tooltip, \n  Legend, \n  BarElement, \n  CategoryScale, \n  LinearScale\n);\n\nexport default {\n  name: 'FiveDayForecast',\n  components: {\n    Bar,\n  },\n  props: {\n    forecastData: {\n      type: Array,\n      default: () => [],\n    },\n    location: {\n      type: String,\n      default: 'Your Location',\n    },\n  },\n  data() {\n    return {\n      chartData: {\n        labels: [], // Days of the week\n        datasets: [\n          {\n            label: 'High Temperature (°F)',\n            data: [], // High temperatures\n            backgroundColor: 'rgba(255, 99, 132, 0.5)',\n            borderColor: 'rgba(255, 99, 132, 1)',\n            borderWidth: 1,\n          },\n          {\n            label: 'Low Temperature (°F)',\n            data: [], // Low temperatures\n            backgroundColor: 'rgba(54, 162, 235, 0.5)',\n            borderColor: 'rgba(54, 162, 235, 1)',\n            borderWidth: 1,\n          }\n        ],\n      },\n      chartOptions: {\n        responsive: true,\n        plugins: {\n          legend: {\n            position: 'top',\n          },\n          tooltip: {\n            callbacks: {\n              label: (tooltipItem) => {\n                return `${tooltipItem.dataset.label}: ${tooltipItem.raw} °F`;\n              },\n            },\n          },\n        },\n        scales: {\n          x: {\n            title: {\n              display: true,\n              text: 'Day',\n            },\n          },\n          y: {\n            title: {\n              display: true,\n              text: 'Temperature (°F)',\n            },\n            beginAtZero: true,\n          },\n        },\n      },\n    };\n  },\n  watch: {\n    forecastData: {\n      immediate: true,\n      handler(newData) {\n        if (newData && newData.length > 0) {\n          this.processForecastData(newData);\n        }\n      }\n    }\n  },\n  methods: {\n    processForecastData(data) {\n      this.chartData.labels = data.map(item => item.day);\n      this.chartData.datasets[0].data = data.map(item => item.high);\n      this.chartData.datasets[1].data = data.map(item => item.low);\n    },\n    goBack() {\n      this.$router.go(-1); // Go back to the previous page\n    },\n  },\n}\n</script>\n\n<style scoped>\n.for