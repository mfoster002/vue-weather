s.errorMessage = \"Error fetching geo data.\";\n      });\n    },\n    dateBuilder() {\n      const now = new Date();\n      const options = {\n        weekday: \"long\",\n        year: \"numeric\",\n        month: \"long\",\n        day: \"numeric\"\n      };\n      return now.toLocaleDateString(\"en-US\", options);\n    },\n    goToMySavedLocations() {\n      this.$router.push({\n        name: \"MySavedLocations\"\n      });\n    },\n    goToHourlyForecast() {\n      if (this.weather.coord) {\n        this.$router.push({\n          name: \"HourlyForecast\",\n          params: {\n            location: this.weather.name\n          },\n          query: {\n            lat: this.weather.coord.lat,\n            lon: this.weather.coord.lon\n          }\n        });\n      } else {\n        this.errorMessage = \"Weather data is incomplete. Please try again.\";\n      }\n    },\n    goToFiveDayForecast() {\n      if (this.weather.name) {\n        this.$router.push({\n          name: \"FiveDayForecast\",\n          params: {\n            location: this.weather.name\n          }\n        });\n      } else {\n        this.errorMessage = \"Weather data is incomplete. Please try again.\";\n      }\n    }\n  },\n  mounted() {\n    const saved = localStorage.getItem(\"savedLocations\");\n    this.savedLocations = saved ? JSON.parse(saved) : [];\n  }\n};","map":{"version":3,"names":["name","data","api_key","url_base","query","weather","geoData","savedLocations","errorMessage","computed","weatherClass","weatherCondition","main","toLowerCase","conditionMap","clear","clouds","rain","drizzle","thunderstorm","snow","mist","methods","fetchWeather","trim","fetch","then","res","json","cod","fetchGeoData","catch","err","console","error","length","state","country","dateBuilder","now","Date","options","weekday","year","month","day","toLocaleDateString","goToMySavedLocations","$router","push","goToHourlyForecast","coord","params","location","lat","lon","goToFiveDayForecast","mounted","saved",