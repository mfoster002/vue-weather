{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'CurrentView',\n  data() {\n    return {\n      api_key: '6c07b7050c30876a4e88cfbb4dee8dc3',\n      url_base: 'https://api.openweathermap.org/data/2.5/',\n      query: '',\n      weather: {},\n      savedLocations: [],\n      errorMessage: ''\n    };\n  },\n  methods: {\n    fetchWeather(e) {\n      if (e.key === 'Enter' && this.query.trim() !== '') {\n        this.errorMessage = ''; // Reset error message before fetching data\n        fetch(`${this.url_base}weather?q=${this.query}&units=imperial&APPID=${this.api_key}`).then(res => res.json()).then(data => {\n          if (data.cod !== 200) {\n            this.errorMessage = 'Location not found. Please try again.'; // Error handling\n            return;\n          }\n          this.weather = data;\n        }).catch(err => {\n          this.errorMessage = 'Error fetching weather data. Please try again later.'; // Catching network errors\n          console.error(err);\n        });\n      }\n    },\n    saveLocation() {\n      if (this.weather.name && !this.savedLocations.includes(this.weather.name)) {\n        this.savedLocations.push(this.weather.name);\n      }\n    },\n    dateBuilder() {\n      const now = new Date();\n      const options = {\n        weekday: 'long',\n        year: 'numeric',\n        month: 'long',\n        day: 'numeric'\n      };\n      return now.toLocaleDateString('en-US', options);\n    },\n    navigateTo5Day() {\n      if (this.weather.name) {\n        this.$router.push('/5day'); // Update as per your router setup\n      } else {\n        this.errorMessage = 'Weather data is missing.';\n      }\n    }\n  }\n};","map":{"version":3,"names":["name","data","api_key","url_base","query","weather","savedLocations","errorMessage","methods","fetchWeather","e","key","trim","fetch","then","res","json","cod","catch","err","console","error","saveLocation","includes","push","dateBuilder","now","Date","options","weekday","year","month","day","toLocaleDateString","navigateTo5Day","$router"],"sources":["/Users/marianfoster/vue-weather/src/views/CurrentView.vue"],"sourcesContent":["<template>\n  <div class=\"current-view\">\n    <!-- Search Bar -->\n    <div class=\"search-box\">\n      <input \n        type=\"text\" \n        class=\"search-bar\" \n        placeholder=\"Search...\"\n        v-model=\"query\"\n        @keypress=\"fetchWeather\" \n      />\n    </div>\n\n    <div v-if=\"errorMessage\" class=\"error-message\">{{ errorMessage }}</div>\n\n    <!-- Current Weather Display -->\n    <div class=\"weather-wrap\" v-if=\"weather.main && !errorMessage\">\n      <div class=\"location-box\">\n        <div class=\"location\">{{ weather.name }}, {{ weather.sys.country }}</div>\n        <div class=\"date\">{{ dateBuilder() }}</div>\n      </div>\n\n      <div class=\"weather-box\">\n        <div class=\"temp\">{{ Math.round(weather.main.temp) }}Â°F</div>\n        <div class=\"weather\">{{ weather.weather[0].main }}</div>\n      </div>\n\n      <!-- Save Location and Forecast Navigation -->\n      <button @click=\"saveLocation\">Save Location</button>\n      <button @click=\"navigateTo5Day\">Go to 5-Day Forecast</button>\n\n      <!-- Saved Locations Section -->\n      <div v-if=\"savedLocations && savedLocations.length\">\n        <h3>Saved Locations:</h3>\n        <ul>\n          <li v-for=\"(location, index) in savedLocations\" :key=\"index\">{{ location }}</li>\n        </ul>  \n      </div>\n    </div>  \n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'CurrentView',\n  data() {\n    return {\n      api_key: '6c07b7050c30876a4e88cfbb4dee8dc3',\n      url_base: 'https://api.openweathermap.org/data/2.5/',\n      query: '',\n      weather: {},\n      savedLocations: [],\n      errorMessage: '',\n    };\n  },\n\n  methods: {\n    fetchWeather(e) {\n      if (e.key === 'Enter' && this.query.trim() !== '') {\n        this.errorMessage = ''; // Reset error message before fetching data\n        fetch(`${this.url_base}weather?q=${this.query}&units=imperial&APPID=${this.api_key}`)\n          .then((res) => res.json())\n          .then((data) => {\n            if (data.cod !== 200) {\n              this.errorMessage = 'Location not found. Please try again.'; // Error handling\n              return;\n            }\n            this.weather = data;\n          })\n          .catch((err) => {\n            this.errorMessage = 'Error fetching weather data. Please try again later.'; // Catching network errors\n            console.error(err);\n          });\n      }\n    },\n\n    saveLocation() {\n      if (this.weather.name && !this.savedLocations.includes(this.weather.name)) {\n        this.savedLocations.push(this.weather.name);\n      }\n    },\n\n    dateBuilder() {\n      const now = new Date();\n      const options = { weekday: 'long', year: 'numeric', month: 'long', day: 'numeric' };\n      return now.toLocaleDateString('en-US', options);\n    },\n\n    navigateTo5Day() {\n      if (this.weather.name) {\n        this.$router.push('/5day'); // Update as per your router setup\n      } else {\n        this.errorMessage = 'Weather data is missing.';\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Keep the same styles from your original App.vue */\n.current-view {\n  text-align: center;\n  padding: 20px;\n}\n\n.search-box {\n  margin-bottom: 30px;\n}\n\n.search-box .search-bar {\n  padding: 15px;\n  font-size: 20px;\n}\n\n.weather-wrap {\n  margin-top: 20px;\n}\n\n.location-box {\n  font-size: 32px;\n}\n\n.weather-box {\n  font-size: 48px;\n}\n\nbutton {\n  margin-top: 20px;\n  padding: 10px;\n  font-size: 16px;\n}\n</style>\n\n"],"mappings":";AA2CA,eAAe;EACbA,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,kCAAkC;MAC3CC,QAAQ,EAAE,0CAA0C;MACpDC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE,CAAC,CAAC;MACXC,cAAc,EAAE,EAAE;MAClBC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EAEDC,OAAO,EAAE;IACPC,YAAYA,CAACC,CAAC,EAAE;MACd,IAAIA,CAAC,CAACC,G