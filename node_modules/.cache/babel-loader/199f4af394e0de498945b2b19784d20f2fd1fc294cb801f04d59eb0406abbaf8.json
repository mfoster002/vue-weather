tn {\n  margin-top: 20px;\n  padding: 10px 20px;\n  background-color: #008CBA;\n  color: white;\n  border: none;\n  border-radius: 5px;\n}\n\n.back-btn:hover {\n  background-color: #007399;\n}\n</style>\n\n\n"],"mappings":"AAuCA,OAAOA,eAAc,MAAO,kCAAkC;AAE9D,eAAe;EACbC,UAAU,EAAE;IAAED;EAAgB,CAAC;EAC/BE,KAAK,EAAE;IACLC,QAAQ,EAAE;MACRC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE,IAAI,CAAE;IAClB;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,MAAMA,CAAA,EAAG;MACP,IAAI,CAACC,OAAO,CAACC,EAAE,CAAC,CAAC,CAAC,CAAC,EAAE;IACvB;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}