forecastData.map((day) => day.low);\n  \n        this.chart = new Chart(ctx, {\n          type: 'bar',\n          data: {\n            labels,\n            datasets: [\n              {\n                label: 'High Temp (°F)',\n                data: highTemps,\n                backgroundColor: 'rgba(255, 99, 132, 0.5)',\n                borderColor: 'rgba(255, 99, 132, 1)',\n                borderWidth: 1,\n              },\n              {\n                label: 'Low Temp (°F)',\n                data: lowTemps,\n                backgroundColor: 'rgba(54, 162, 235, 0.5)',\n                borderColor: 'rgba(54, 162, 235, 1)',\n                borderWidth: 1,\n              },\n            ],\n          },\n          options: {\n            responsive: true,\n            plugins: {\n              legend: {\n                position: 'top',\n              },\n            },\n            scales: {\n              y: {\n                beginAtZero: true,\n                title: {\n                  display: true,\n                  text: 'Temperature (°F)',\n                },\n              },\n            },\n          },\n        });\n      },\n      goBack() {\n        this.$router.back();\n      },\n    },\n    mounted() {\n      this.drawChart();\n    },\n  };\n  </script>\n  \n  <style scoped>\n  .five-day-forecast {\n    text-align: center;\n    margin: 20px;\n  }\n  \n  .weather-chart {\n    max-width: 600px;\n    margin: 0 auto;\n  }\n  </style>\n  "],"mappings":";;EACSA,KAAK,EAAC;AAAmB;;EAEpBC,GAAG,EAAC,OAAO;EAACD,KAAK,EAAC;;;uBAF5BE,mBAAA,CAIM,OAJNC,UAIM,GAHJC,mBAAA,CAAkD,YAA9C,6BAA2B,GAAAC,gBAAA,CAAGC,MAAA,CAAAC,QAAQ,kBAC1CH,mBAAA,CAAmD,UAAnDI,UAAmD,+BACnDJ,mBAAA,CAAqC;IAA5BK,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAC,MAAA,IAAAD,QAAA,CAAAC,MAAA,IAAAF,IAAA,CAAM;KAAE,MAAI,E","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}