iv>\n    <div v-else>\n      <p>Loading forecast...</p>\n    </div>\n    <button class=\"back-button\" @click=\"goBack\">Back</button>\n  </div>\n</template>\n\n<script>\nimport { Bar } from 'vue-chartjs';\nimport {\n  Chart as ChartJS,\n  Title,\n  Tooltip,\n  Legend,\n  BarElement,\n  CategoryScale,\n  LinearScale,\n} from 'chart.js';\n\nChartJS.register(\n  Title, \n  Tooltip, \n  Legend, \n  BarElement, \n  CategoryScale, \n  LinearScale\n);\n\nexport default {\n  name: 'FiveDayForecast',\n  components: {\n    Bar,\n  },\n  props: {\n    forecastData: {\n      type: Array,\n      default: () => [],\n    },\n    location: {\n      type: String,\n      default: 'Your Location',\n    },\n  },\n  data() {\n    return {\n      chartData: {\n        labels: [], // Days of the week\n        datasets: [\n          {\n            label: 'High Temperature (째F)',\n            data: [], // High temperatures\n            backgroundColor: 'rgba(255, 99, 132, 0.5)',\n            borderColor: 'rgba(255, 99, 132, 1)',\n            borderWidth: 1,\n          },\n          {\n            label: 'Low Temperature (째F)',\n            data: [], // Low temperatures\n            backgroundColor: 'rgba(54, 162, 235, 0.5)',\n            borderColor: 'rgba(54, 162, 235, 1)',\n            borderWidth: 1,\n          }\n        ],\n      },\n      chartOptions: {\n        responsive: true,\n        plugins: {\n          legend: {\n            position: 'top',\n          },\n          tooltip: {\n            callbacks: {\n              label: (tooltipItem) => {\n                return `${tooltipItem.dataset.label}: ${tooltipItem.raw} 째F`;\n              },\n            },\n          },\n        },\n        scales: {\n          x: {\n            title: {\n              display: true,\n              text: 'Day',\n            },\n          },\n          y: {\n            title: {\n              display: true,\n              text: 'Temperature (째F)',\n            },\n            beginAtZero: true,\n          },\n        },\n      },\n    };\n  },\n  watch: {\n    forecastData: {\n      immediate: true,\n      handler(newData) {\n        if (newData && newData.length > 0) {\n          this.processForecastData(newData);\n        }\n      }\n    }\n  },\n  methods: {\n    processForecastData(data) {\n      this.chartData.labels = data.map(item => item.day);\n      this.chartData.datasets[0].data = data.map(item => item.high);\n      this.chartData.datasets[1].data = data.map(item => item.low);\n    },\n    goBack() {\n      this.$router.go(-1); // Go back to the previous page\n    },\n  },\n}\n</script>\n\n<style scoped>\n.forecast-container {\n  padding: 20px;\n  text-align: center;\n  background-color: #f0f8ff;\n  border-radius: 8px;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n  margin: 20px auto;\n  max-width: 900px;\n}\n\nh2 {\n  margin-bottom: 20px;\n  font-size: 1.8em;\n  color: #333;\n}\n\ncanvas {\n  max-width: 100%;\n  height: 400px;\n}\n\n.loading-message {\n  font-size: 1.2em;\n  color: #666;\n  font-style: italic;\n}\n\n.back-button {\n  margin-top: 20px;\n  padding: 10px 20px;\n  font-size: 1em;\n  background-color: #4CAF50;\n  color: white;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.back-button:hover {\n  background-color: #45a049;\n}\n</style>\n\n"],"mappings":";;EACOA,KAAK,EAAC;AAAoB;;EADjCC,GAAA;AAAA;;EAAAA,GAAA;AAAA;;;uBACEC,mBAAA,CAYM,OAZNC,UAYM,GAXJC,mBAAA,CAAwC,YAAAC,gBAAA,CAAjCC,MAAA,CAAAC,QAAQ,IAAG,mBAAiB,iBACxBD,MAAA,CAAAE,YAAY,IAAIF,MAAA,CAAAE,YAAY,CAACC,MAAM,Q,cAA9CP,mBAAA,CAKM,OARVQ,UAAA,GAIMC,YAAA,CAGEC,cAAA;IAFCC,IAAI,EAAEC,KAAA,CAAAC,SAAS;IACfC,OAAO,EAAEF,KAAA,CAAAG;mEAGdf,mBAAA,CAEM,OAXVgB,UAAA,EAAAC,MAAA,QAAAA,MAAA,OAUMf,mBAAA,CAA0B,WAAvB,qBAAmB,oB,KAExBA,mBAAA,CAAyD;IAAjDJ,KAAK,EAAC,aAAa;IAAEoB,OAAK,EAAAD,MAAA,QAAAA,MAAA,UAAAE,IAAA,KAAEC,QAAA,CAAAC,MAAA,IAAAD,QAAA,CAAAC,MAAA,IAAAF,IAAA,CAAM;KAAE,MAAI,E","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}