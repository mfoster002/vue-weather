{"ast":null,"code":"import \"core-js/modules/esnext.iterator.map.js\";\nimport { Bar } from 'vue-chartjs';\nimport { Chart as ChartJS, Title, Tooltip, Legend, BarElement, CategoryScale, LinearScale } from 'chart.js';\n\n// Register Chart.js components\nChartJS.register(Title, Tooltip, Legend, BarElement, CategoryScale, LinearScale);\nexport default {\n  name: 'FiveDayForecast',\n  components: {\n    Bar\n  },\n  props: {\n    location: {\n      type: String,\n      required: true\n    }\n  },\n  data() {\n    return {\n      forecastData: [],\n      chartData: {\n        labels: [],\n        datasets: [{\n          label: 'Daily Temperature (째F)',\n          data: [],\n          backgroundColor: 'rgba(75, 192, 192, 0.6)',\n          borderColor: 'rgba(75, 192, 192, 1)',\n          borderWidth: 1\n        }]\n      },\n      chartOptions: {\n        responsive: true,\n        plugins: {\n          legend: {\n            display: true,\n            position: 'top'\n          },\n          title: {\n            display: true,\n            text: '5-Day Temperature Forecast'\n          }\n        },\n        scales: {\n          x: {\n            title: {\n              display: true,\n              text: 'Day'\n            }\n          },\n          y: {\n            title: {\n              display: true,\n              text: 'Temperature (째F)'\n            },\n            beginAtZero: true\n          }\n        }\n      }\n    };\n  },\n  watch: {\n    location: {\n      immediate: true,\n      handler(newLocation) {\n        if (newLocation) {\n          this.fetchForecastData();\n        } else {\n          console.error('Location is missing or undefined.');\n        }\n      }\n    }\n  },\n  methods: {\n    fetchForecastData() {\n      if (this.location) {\n        fetch(`https://api.openweathermap.org/data/2.5/forecast?q=${this.location}&units=imperial&cnt=5&appid=6c07b7050c30876a4e88cfbb4dee8dc3`).then(res => res.json()).then(data => {\n          if (data.cod !== '200') {\n            console.error('Unable to fetch forecast data.');\n            return;\n          }\n          this.forecastData = data.list.map(item => ({\n            date: new Date(item.dt * 1000).toLocaleDateString(),\n            temp: item.main.temp\n          }));\n          this.updateChartData(); // Update chart data after fetching\n        }).catch(err => {\n          console.error('Error fetching forecast data:', err);\n        });\n      } else {\n        console.error('Location is missing or undefined.');\n      }\n    },\n    updateChartData() {\n      this.chartData.labels = this.forecastData.map(item => item.date);\n      this.chartData.datasets[0].data = this.forecastData.map(item => item.temp);\n    },\n    goBack() {\n      this.$router.go(-1); // Go back to the previous page\n    }\n  }\n};","map":{"version":3,"names":["Bar","Chart","ChartJS","Title","Tooltip","Legend","BarElement","CategoryScale","LinearScale","register","name","components","props","location","type","String","required","data","forecastData","chartData","labels","datasets","label","backgroundColor","borderColor","borderWidth","chartOptions","responsive","plugins","legend","display","position","title","text","scales","x","y","beginAtZero","watch","immediate","handler","newLocation","fetchForecastData","console","error","methods","fetch","then","res","json","cod","list","map","item","date","Date","dt","toLocaleDateString","temp","main","updateChartData","catch","err","goBack","$router","go"],"sources":["/Users/marianfoster/vue-weather/src/views/5Day.vue"],"sourcesContent":["<template>\n  <div class=\"forecast-container\">\n    <h2>{{ location }}'s 5-Day Forecast</h2>\n\n    <button @click=\"fetchForecastData\">Show 5-day Forecast</button>\n    \n    <div v-if=\"forecastData && forecastData.length > 0\">\n      <Bar \n        :data=\"chartData\" \n        :options=\"chartOptions\" \n      />\n    </div>\n    <div v-else>\n      <p>Loading forecast...</p>\n    </div>\n\n    <button class=\"back-button\" @click=\"goBack\">Back</button>\n  </div>\n</template>\n\n<script>\nimport { Bar } from 'vue-chartjs';\nimport {\n  Chart as ChartJS,\n  Title,\n  Tooltip,\n  Legend,\n  BarElement,\n  CategoryScale,\n  LinearScale,\n} from 'chart.js';\n\n// Register Chart.js components\nChartJS.register(\n  Title, \n  Tooltip, \n  Legend, \n  BarElement, \n  CategoryScale, \n  LinearScale\n);\n\nexport default {\n  name: 'FiveDayForecast',\n  components: {\n    Bar,\n  },\n  props: {\n    location: {\n      type: String,\n      required: true,\n    },\n  },\n  data() {\n    return {\n      forecastData: [],\n      chartData: {\n        labels: [],\n        datasets: [\n          {\n            label: 'Daily Temperature (째F)',\n            data: [],\n            backgroundColor: 'rgba(75, 192, 192, 0.6)',\n            borderColor: 'rgba(75, 192, 192, 1)',\n            borderWidth: 1,\n          },\n        ],\n      },\n      chartOptions: {\n        responsive: true,\n        plugins: {\n          legend: {\n            display: true,\n            position: 'top',\n          },\n          title: {\n            display: true,\n            text: '5-Day Temperature Forecast',\n          },\n        },\n        scales: {\n          x: {\n            title: {\n              display: true,\n              text: 'Day',\n            },\n          },\n          y: {\n            title: {\n              display: true,\n              text: 'Temperature (째F)',\n            },\n            beginAtZero: true,\n          },\n        },\n      },\n    };\n  },\n  watch: {\n    location: {\n      immediate: true,\n      handler(newLocation) {\n        if (newLocation) {\n          this.fetchForecastData();\n        } else {\n          console.error('Location is missing or undefined.');\n        }\n      }\n    }\n  },\n  methods: {\n    fetchForecastData() {\n      if (this.location) {\n        fetch(\n          `https://api.openweathermap.org/data/2.5/forecast?q=${this.location}&units=imperial&cnt=5&appid=6c07b7050c30876a4e88cfbb4dee8dc3`\n        )\n          .then((res) => res.json())\n          .then((data) => {\n            if (data.cod !== '200') {\n              console.error('Unable to fetch forecast data.');\n              return;\n            }\n            this.forecastData = data.list.map((item) => ({\n              date: new Date(item.dt * 1000).toLocaleDateString(),\n              temp: item.main.temp,\n            }));\n            this.updateChartData(); // Update chart data after fetching\n          })\n          .catch((err) => {\n            console.error('Error fetching forecast data:', err);\n          });\n      } else {\n        console.error('Location is missing or undefined.');\n      }\n    },\n\n    updateChartData() {\n      this.chartData.labels = this.forecastData.map((item) => item.date);\n      this.chartData.datasets[0].data = this.forecastData.map((item) => item.temp);\n    },\n\n    goBack() {\n      this.$router.go(-1); // Go back to the previous page\n    },\n  },\n}\n</script>\n\n\n<style scoped>\nbody {\n  background-color: #f0f8ff;\n}\n.forecast-container {\n  padding: 20px;\n  text-align: center;\n  background-color: #f0f8ff;\n  border-radius: 8px;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n  margin: 20px auto;\n  max-width: 900px;\n}\n\nh2 {\n  margin-bottom: 20px;\n  font-size: 1.8em;\n  color: #333;\n}\n\ncanvas {\n  max-width: 100%;\n  height: auto;\n}\n\n.loading-message {\n  font-size: 1.2em;\n  color: #666;\n  font-style: italic;\n}\n\n.back-button {\n  background-color: #4CAF50;\n  color: white;\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n  display: inline-block;\n  position: relative;\n  z-index: 100; \n  margin-top: 20px;\n}\n\n.back-button:hover {\n  background-color: #45a049;\n}\n</style>\n\n\n"],"mappings":";AAqBA,SAASA,GAAE,QAAS,aAAa;AACjC,SACEC,KAAI,IAAKC,OAAO,EAChBC,KAAK,EACLC,OAAO,EACPC,MAAM,EACNC,UAAU,EACVC,aAAa,EACbC,WAAW,QACN,UAAU;;AAEjB;AACAN,OAAO,CAACO